#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/athul/Desktop/ExpressJS-CleanArchitecture/node_modules/.pnpm/typeorm-ts-node-commonjs@0.3.20/node_modules/typeorm-ts-node-commonjs/node_modules:/home/athul/Desktop/ExpressJS-CleanArchitecture/node_modules/.pnpm/typeorm-ts-node-commonjs@0.3.20/node_modules:/home/athul/Desktop/ExpressJS-CleanArchitecture/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/athul/Desktop/ExpressJS-CleanArchitecture/node_modules/.pnpm/typeorm-ts-node-commonjs@0.3.20/node_modules/typeorm-ts-node-commonjs/node_modules:/home/athul/Desktop/ExpressJS-CleanArchitecture/node_modules/.pnpm/typeorm-ts-node-commonjs@0.3.20/node_modules:/home/athul/Desktop/ExpressJS-CleanArchitecture/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir//bin/sh" ]; then
  exec "$basedir//bin/sh"  "$basedir/../../wrapper.sh" "$@"
else
  exec /bin/sh  "$basedir/../../wrapper.sh" "$@"
fi
